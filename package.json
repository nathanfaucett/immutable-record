    "jshint": "^2.9.4",
    "immutable": "^3.8.1",
    "mori": "^0.3.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "zuul": "^3.11.1"
  },
  "dependencies": {
    "@nathanfaucett/define_property": "^0.0.3",
    "@nathanfaucett/freeze": "^0.0.1",
