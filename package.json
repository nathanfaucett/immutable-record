{
  "name": "immutable-record",
  "version": "0.0.1",
  "description": "immutable persistent record for the browser and node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/immutable-record.git"
  },
  "main": "src/index.js",
  "scripts": {
    "bench": "node bench",
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "immutable-record",
    "immutable-record.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-jsbeautifier": "^0.2.10",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2",
    "zuul": "^3.7.2"
  },
  "dependencies": {
    "define_property": "git://github.com/nathanfaucett/define_property.git",
    "freeze": "git://github.com/nathanfaucett/freeze.git",
    "has": "git://github.com/nathanfaucett/has.git",
    "immutable-hash_map": "git://github.com/nathanfaucett/immutable-hash_map.git",
    "inherits": "git://github.com/nathanfaucett/inherits.git",
    "keys": "git://github.com/nathanfaucett/keys.git"
  }
}
