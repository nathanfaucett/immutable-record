    "tape": "^4.2.2",
    "zuul": "^3.7.2"
  },
  "dependencies": {
    "@nathanfaucett/define_property": "^0.0.1",
    "@nathanfaucett/freeze": "^0.0.1",
    "@nathanfaucett/has": "^0.0.1",
    "@nathanfaucett/immutable-hash_map": "^0.0.1",
